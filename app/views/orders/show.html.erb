<div class="container">
  <table border="" cellspacing="10" cellpadding="10">
    <tr><th>Order <%= @order.id %></th></tr>
    <tr><th>Item Image</th>
    <th>Item Name</th>
    <th>User</th>
    <% @order.items.each do |item| %>
      <tr>
        <td><%= image_tag(item.image_url) %></td>
        <td> <%= item.name %></td>
        <td> <%= @order.user.name %></td>
      </tr>
    <% end %>
  </table>
</div>
